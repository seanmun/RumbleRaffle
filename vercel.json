{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "backend/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.ts"
    }
  ],
  "functions": {
    "api/wrestlers": {
      "runtime": "nodejs18.x"
    }
  }
}
